{
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "commonjs": true
  },

  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-continue": "warn",
    "no-param-reassign": "warn"
  }
}
